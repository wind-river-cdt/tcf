      <!-- Memory View -->
      <menuContribution
            locationURI="toolbar:org.eclipse.debug.ui.MemoryView?after=additions">
         <command
               commandId="org.eclipse.tcf.debug.ui.commands.refresh"
               icon="icons/refresh.gif"
